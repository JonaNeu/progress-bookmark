{
    "name": "site-progress-bookmark",
    "version": "1.0",
    "manifest_version": 2,
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"],
        "run_at": "document_end"
      }
    ],
    "browser_action": {
      "default_popup": "popup.html",
      "default_title": "Site Progress Bookmark"
    },
    "permissions": [
        "storage", "tabs"
    ]
  }